{
  "functions": {
    "api/anthropic.js": { "maxDuration": 30 },
    "api/elevenlabs.js": { "maxDuration": 30 }
  },
  "rewrites": [
    { "source": "/api/anthropic/v1/:path*", "destination": "/api/anthropic" },
    { "source": "/api/elevenlabs/:path*",   "destination": "/api/elevenlabs" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite"
}
